<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.djcao.boot.repository.User">
        <table name="user" schema="sneakerlover" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="20"/>
            </id>
            <basic name="createTime">
                <column name="create_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="updateTime">
                <column name="update_time" nullable="false" precision="-1"/>
            </basic>
            <basic name="outerId">
                <column name="outer_id" nullable="false" length="80"/>
            </basic>
            <basic name="userName">
                <column name="user_name" nullable="false" length="80"/>
            </basic>
            <basic name="passwd">
                <column name="passwd" nullable="false"/>
            </basic>
            <basic name="isVip">
                <column name="is_vip" nullable="false" precision="4"/>
            </basic>
            <basic name="ext">
                <column name="ext" nullable="false"/>
            </basic>
            <basic name="email">
                <column name="email" nullable="false"/>
            </basic>
            <basic name="phoneNum">
                <column name="phone_num" nullable="false" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
